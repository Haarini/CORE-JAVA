import java.util.*;
import java.lang.*;
import java.util.Scanner;
//Function with no arguements and with return types
class add_nawr
{  
    int add()
    {
        int a,b;
        Scanner s=new Scanner(System.in);
        System.out.println("Enter numbers to be added:");
        a=s.nextInt();
        b=s.nextInt();
        return(a+b);
       
    }
        
    public static void main(String args[])
    {
      
        Rextester o=new Rextester();
        System.out.println("Sum is "+o.add());
       
    }
}
